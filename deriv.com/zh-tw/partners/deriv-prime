<!-- Trackjs - end of code --><script>
    var Webflow = Webflow || [];
    Webflow.push(function() {
        // Get elements
        let inactiveTabPane = document.querySelector('.inactive-tab-pane');
        let inactiveTab = document.querySelector('.inactive-tab');

        // Add Event Listener
        inactiveTab.addEventListener('click', openAccordion);

        function openAccordion() {
            setTimeout(() => {
                // Click accordion item
                inactiveTabPane.querySelector('.accordion_toggle-block').click();
            }, 300);
            // Remove Event Listener
            inactiveTab.removeEventListener('click', openAccordion);
        }
    });

    document.addEventListener("DOMContentLoaded", function() {
        // Prevent form submission by default - start
        let primeForm = document.getElementById('prime-form');
        primeForm.addEventListener('submit', function(event) {
            event.preventDefault();
            event.stopPropagation();
        });
        // Prevent form submission by default - end

        // Get input elements
        const fullNameInput = document.getElementsByName('full-name')[0];
        const companyNameInput = document.getElementsByName('company-name')[0];
        const emailInput = document.getElementsByName('corporate-email')[0];
        const mobileNumberInput = document.getElementsByName('mobile-number')[0];

        // Add onchange event listeners
        fullNameInput.addEventListener('input', handleInputValidation);
        companyNameInput.addEventListener('input', handleInputValidation);
        emailInput.addEventListener('input', handleInputValidation);
        mobileNumberInput.addEventListener('input', handleInputValidation);

        // Function to disable the submit button
        function disableButton() {
            submitBtn.disabled = true;
            submitBtn.style.opacity = '0.5';
        }

        // Function to enable the submit button
        function enableButton() {
            submitBtn.disabled = false;
            submitBtn.style.opacity = '1';
        }

        function toggleFormAndSuccessMessage() {
            // Hide the form and show success message
            document.getElementById("prime-form").style.display = "none";
            document.getElementById("contact-success").style.display = "flex";

            // After 3 seconds, toggle back to form
            setTimeout(() => {
                document.getElementById("prime-form").style.display = "block";
                document.getElementById("contact-success").style.display = "none";
            }, 3000);
        }

        function toggleFormAndErrorMessage() {
            // Hide the form and show error message
            document.getElementById("prime-form").style.display = "none";
            document.getElementById("contact-error").style.display = "flex";

            // After 3 seconds, toggle back to form
            setTimeout(() => {
                document.getElementById("prime-form").style.display = "block";
                document.getElementById("contact-error").style.display = "none";
            }, 5000);
        }

        // Function to handle input validation
        function handleInputValidation(event) {
            const input = event.target;
            const inputValue = input.value.trim();
            const errorText = document.getElementById(input.name + '-error-text');
            const errorIcon = document.getElementById(input.name + '-error-icon');
            const isEmailField = input.name === 'corporate-email';

            // Function to remove error-field class
            function removeErrorClass() {
                if (input) input.classList.remove("error-field");
                if (errorText) errorText.classList.remove("error-field");
                if (errorIcon) errorIcon.classList.remove("error-field");
            }

            // Function to add error-field class
            function addErrorClass() {
                if (input) input.classList.add("error-field");
                if (errorText) errorText.classList.add("error-field");
                if (errorIcon) errorIcon.classList.add("error-field");
            }

            // Validate email format if input is the email field
            if (isEmailField) {
                const emailPattern = window.emailRegex;
                const isValidEmail = emailPattern.test(inputValue);
                isValidEmail ? removeErrorClass() : addErrorClass();
            } else {
                // Remove error-field class if input is not empty
                inputValue !== '' ? removeErrorClass() : addErrorClass();
            }
        }

        var submitBtn = document.getElementById('submit-btn');
        const contact_us_form_post_data = 'https://formkeep.com/f/7f4b8b55f10f';

        submitBtn.addEventListener('click', async function(event) {
            disableButton();
            event.stopPropagation();
            event.preventDefault();

            const fullName = document.getElementsByName('full-name')[0]?.value.trim();
            const companyName = document.getElementsByName('company-name')[0]?.value.trim();
            const email = document.getElementsByName('corporate-email')[0]?.value.trim();
            const mobileNumber = document.getElementsByName('mobile-number')[0]?.value.trim();

            const emailPattern = window.emailRegex;
            if (fullName === '') {
                var fullNameElement = document.getElementById("full-name");
                var fullnameErrorText = document.getElementById("full-name-error-text");
                var fullnameErrorIcon = document.getElementById("full-name-error-icon");
                if (fullNameElement) fullNameElement.classList.add("error-field");
                if (fullnameErrorText) fullnameErrorText.classList.add("error-field");
                if (fullnameErrorIcon) fullnameErrorIcon.classList.add("error-field");
            }

            if (companyName === '') {
                var companyNameElement = document.getElementById("company-name");
                var companyNameErrorText = document.getElementById("company-name-error-text");
                var companyNameErrorIcon = document.getElementById("company-name-error-icon");
                if (companyNameElement) companyNameElement.classList.add("error-field");
                if (companyNameErrorText) companyNameErrorText.classList.add("error-field");
                if (companyNameErrorIcon) companyNameErrorIcon.classList.add("error-field");
            }

            if (!emailPattern.test(email)) {
                var emailElement = document.getElementById("corporate-email");
                var emailErrorText = document.getElementById("corporate-email-error-text");
                var emailErrorIcon = document.getElementById("corporate-email-error-icon");
                if (emailElement) emailElement.classList.add("error-field");
                if (emailErrorText) emailErrorText.classList.add("error-field");
                if (emailErrorIcon) emailErrorIcon.classList.add("error-field");
            }

            if (mobileNumber === '') {
                var mobileNumberElement = document.getElementById("mobile-number");
                var mobileNumberErrorText = document.getElementById("mobile-number-error-text");
                var mobileNumberErrorIcon = document.getElementById("mobile-number-error-icon");
                if (mobileNumberElement) mobileNumberElement.classList.add("error-field");
                if (mobileNumberErrorText) mobileNumberErrorText.classList.add("error-field");
                if (mobileNumberErrorIcon) mobileNumberErrorIcon.classList.add("error-field");
            }

            if (fullName === '' || companyName === '' || !emailPattern.test(email) || mobileNumber === '') {
                enableButton();
                return;
            }

            // Construct data object
            const data = {
                full_name: fullName,
                company_name: companyName,
                email: email,
                mobile_number: mobileNumber
            };

            // Call API
            try {
                const response = await fetch(contact_us_form_post_data, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(data),
                });

                if (response.ok) {
                    document.getElementById("prime-form").reset();
                    enableButton();
                    // Show success
                    toggleFormAndSuccessMessage();
                } else {
                    enableButton();
                    // Show error message
                    toggleFormAndErrorMessage();
                }
            } catch (error) {
                // Re-enable the button in case of error
                enableButton();
                // Show error message
                toggleFormAndErrorMessage();
            }
        });
    });

    // this code is intended to scroll page when dropdown opens
    var Webflow = Webflow || [];
    Webflow.push(async function() {
        let desctopToggles = document.querySelectorAll('.accordion_toggle-block');

        for (let desctopToggle of desctopToggles) {
            desctopToggle.addEventListener('click', (event) => scrollToCurrentToggle(event));
        }

        function scrollToCurrentToggle(event) {
            var closestDropdown = event.target.closest('.w-dyn-item');
            if (closestDropdown.querySelector('.accordion_dropdown').classList.contains('is-active-accordion')) {
                return;
            }
            var previousDropdown = closestDropdown.previousElementSibling;

            if (!previousDropdown) {
                previousDropdown = event.target.closest('.w-tabs');
            }

            setTimeout(() => {
                previousDropdown.scrollIntoView({ behavior: "smooth" });
            }, 100);
        }
    });
</script></body></html>